{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "Maui",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The name of the project to add Mantis UI to",
      "x-prompt": "What's the name of the project you want to add Mantis UI to?"
    },
    "overwrite": {
      "type": "boolean",
      "description": "Overwrite existing files",
      "x-prompt": "Overwrite existing files?",
      "default": false
    }
  },
  "required": ["project"]
}
